{
   "background": {
      "service_worker": "bg.js"
   },
   "action": {
      "default_icon": "SigIconLightBlue16.png",
      "default_title": "Options"
   },
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "description": "A neat and clean way to organize your downloads into folders based on website URLs and file types.",
   "icons": {
      "128": "SigIconLightBlue128.png",
      "16": "SigIconLightBlue16.png",
      "48": "SigIconLightBlue48.png"
   },
   "manifest_version": 3,
   "minimum_chrome_version": "88",
   "name": "MAID V2",
   "options_page": "options.html",
   "permissions": [ "downloads", "storage", "tabs" ],
   "host_permissions": [ "*://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.6.1"
}
